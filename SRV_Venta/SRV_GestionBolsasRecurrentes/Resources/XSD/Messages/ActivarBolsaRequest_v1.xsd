<?xml version = '1.0' encoding = 'UTF-8'?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns="http://entel.com.pe/GestionBolsasRecurrentes_ActivarBolsa_request" 
            targetNamespace="http://entel.com.pe/GestionBolsasRecurrentes_ActivarBolsa_request" 
            elementFormDefault="qualified" 
            xmlns:nxsd="http://xmlns.oracle.com/pcbpel/nxsd" 
            nxsd:version="JSON" 
            nxsd:encoding="UTF-8">
            
   <xsd:element name="activarBolsaRequest">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="header">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="canal" type="xsd:string"/>
                     <xsd:element name="idAplicacion" type="xsd:integer"/>
                     <xsd:element name="usuario" type="xsd:string"/>
                     <xsd:element name="idTransaccionESB" type="xsd:integer"/>
                     <xsd:element name="idTransaccionNegocio" type="xsd:integer"/>
                     <xsd:element name="fechaInicio" type="xsd:string"/>
                     <xsd:element name="nodoAdicional" type="xsd:string"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="body">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="idCliente" type="xsd:integer"/>
                     <xsd:element name="numeroTelefono" type="xsd:string"/>
                     <xsd:element name="idServicio" type="xsd:integer"/>
                     <xsd:element name="servicioPrecio" type="xsd:double"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
   
<xsd:annotation xmlns="">
      <xsd:appinfo>NXSDSAMPLE=</xsd:appinfo>
      <xsd:appinfo>USEHEADER=false</xsd:appinfo>
   </xsd:annotation>
</xsd:schema>